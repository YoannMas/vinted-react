a] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n  const [page, setPage] = useState(1);\n  const [limit, setLimit] = useState(10);\n  let sort = \"price-asc\";\n\n  if (!price) {\n    sort = \"price-desc\";\n  }\n  console.log(sort);\n  // const numPages = Math.ceil(data.number / limit); // count the number of pages, adding buttons with pages number\n  console.log(search);\n\n  const fetchData = async () => {\n    try {\n      const response = await axios.get(\n        `https://vinted-reacteur.herokuapp.com/offers?page=${page}&title=${search}&sort=${sort}&priceMin=${range[0]}&priceMax=${range[1]}&limit=${limit}`\n      );\n      setData(response.data);\n      setIsLoading(false);\n      setMainPage(true);\n    } catch (error) {\n      console.log(error.message);\n      if (error.response) {\n        console.log(error.response.message);\n      }\n    }\n  };\n\n  console.log(data);\n\n  useEffect(() => {\n    fetchData();\n  }, [page, search, sort, range, limit]);\n\n  return isLoading ? (\n    <span>En cours de chargement</span>\n  ) : (\n    <div className=\"home\">\n      <div className=\"hero\">\n        <img src={hero} alt=\"Someone buying clothes\" />\n        <img className=\"overlay\" src={overlay} alt=\"overlay\" />\n        <div className=\"hero-box\">\n          <h3>Prêts à faire du tri dans vos placard ?</h3>\n          <Link to=\"#\">Commencer à vendre</Link>\n        </div>\n      </div>\n      <div className=\"container\">\n        <div className=\"select\">\n          <span>Nombre de produits par page :</span>\n          <select\n            onChange={(event) => {\n              setLimit(Number(event.target.value));\n            }}\n          >\n            <option value=\"10\">10</option>\n            <option value=\"25\">25</option>\n            <option value=\"50\">50</option>\n          </select>\n        </div>\n        <div className=\"products\">\n          {data.offers.map((el) => {\n            return <Product data={el} key={el._id} />;\n          })}\n        </div>\n      </div>\n      <div className=\"pages\">\n        {/* to improve with number of page */}\n        {page > 1 ? (\n          <button\n            onClick={() => {\n              setPage(page - 1);\n            }}\n          >\n            <FontAwesomeIcon icon={faChevronLeft} style={{ fontSize: 20, marginRight: 20 }} />\n            Précedent\n          </button>\n        ) : (\n          <div className=\"first-hidden\"></div>\n        )}\n        {data.offers.length < limit ? (\n          <div className=\"second-hidden\"></div>\n        ) : (\n          <button\n            onClick={() => {\n              setPage(page + 1);\n            }}\n          >\n            Suivant\n            <FontAwesomeIcon icon={faChevronRight} style={{ fontSize: 20, marginLeft: 20 }} />\n          </button>\n        )}\n      </div>\n    </div>\n  );\n};\nexport default Home;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Offer.js",["54","55"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Header.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Product.js",["56"],"import { Link } from \"react-router-dom\";\nimport photoTest from \"../assets/img/aboutme.jpg\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\n\nconst Product = ({ data }) => {\n  return (\n    <div className=\"product\">\n      <div className=\"user-infos\">\n        {/* Prod */}\n        {data.owner.account.avatar ? (\n          <img className=\"profil-picture\" src={data.owner.account.avatar.secure_url} alt=\"User's avatar\" />\n        ) : (\n          <div className=\"userpic\"></div>\n        )}\n        {/* Test */}\n        {/* <img className=\"profil-picture\" src={photoTest} alt=\"User's avatar\" /> */}\n        <span>{data.owner.account.username}</span>\n      </div>\n      <Link to={`/Offer/${data._id}`}>\n        <img src={data.product_image.secure_url} alt={`${data.product_name}`} />\n      </Link>\n      <div className=\"product-infos\">\n        <div>\n          <span>{data.product_price.toFixed(2)} €</span>\n          <span>{data.product_details[1].TAILLE}</span>\n          <span>{data.product_details[0].MARQUE}</span>\n        </div>\n        <FontAwesomeIcon icon=\"heart\" style={{ color: \"lightgrey\" }} />\n      </div>\n    </div>\n  );\n};\n\nexport default Product;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/ProductDetails.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/PriceSwitch.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/PriceRange.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Signup.js",["57"],"import { useState } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faTimes } from \"@fortawesome/free-solid-svg-icons\";\nimport axios from \"axios\";\n\nconst Signup = ({ setUser, setSignupModal, setLoginModal }) => {\n  const [username, setUsername] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [phone, setPhone] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [errorMessage, setErrorMessage] = useState(\"\");\n  const history = useHistory();\n\n  const handleSubmit = async (event) => {\n    try {\n      event.preventDefault();\n      const response = await axios.post(\"https://vinted-reacteur.herokuapp.com/user/signup\", {\n        username: username,\n        email: email,\n        phone: phone,\n        password: password,\n      });\n      setUser(response.data.token);\n      setSignupModal(false);\n      history.push(\"/\");\n    } catch (error) {\n      console.log(error.message);\n      setErrorMessage(\"There is something wrong, please try again\");\n      if (error.response) {\n        console.log(error.response.message);\n      }\n    }\n  };\n\n  return (\n    <div className=\"modals\">\n      <div className=\"signup-login\">\n        <span\n          onClick={() => {\n            setSignupModal(false);\n          }}\n        >\n          <FontAwesomeIcon icon={faTimes} />\n        </span>\n        <h2>S'inscrire</h2>\n        <form onSubmit={handleSubmit}>\n          <input\n            type=\"text\"\n            value={username}\n            placeholder=\"Nom d'utilisateur\"\n            onChange={(event) => {\n              setUsername(event.target.value);\n            }}\n          />\n          <input\n            type=\"email\"\n            value={email}\n            placeholder=\"Email\"\n            onChange={(event) => {\n              setEmail(event.target.value);\n            }}\n          />\n          <input\n            type=\"tel\"\n            value={phone}\n            placeholder=\"Numéro de téléphone\"\n            onChange={(event) => {\n              setPhone(event.target.value);\n            }}\n          />\n          <input\n            type=\"password\"\n            value={password}\n            placeholder=\"Mot de passe\"\n            onChange={(event) => {\n              setPassword(event.target.value);\n            }}\n          />\n          <div className=\"newsletter\">\n            <input type=\"checkbox\" name=\"newsletter\" />\n            <label>S'inscrire à notre newsletter</label>\n          </div>\n          <p>\n            En m'inscrivant je confirme avoir lu et accepté les Termes & Conditions et Politique de Confidentialité de Vinted. Je confirme\n            avoir au moins 18 ans.\n          </p>\n          <button type=\"submit\">S'inscrire</button>\n          <span style={{ fontSize: 12, color: \"red\", display: \"flex\", justifyContent: \"center\", marginBottom: 10 }}>{errorMessage}</span>\n          <a\n            onClick={() => {\n              setSignupModal(false);\n              setLoginModal(true);\n            }}\n          >\n            Tu as déj