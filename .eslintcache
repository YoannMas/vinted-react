port { Link } from \"react-router-dom\";\nimport axios from \"axios\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faChevronLeft, faChevronRight } from \"@fortawesome/free-solid-svg-icons\";\n\nconst Home = ({ search, price, range, setMainPage }) => {\n  const [data, setData] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n  const [page, setPage] = useState(1);\n  const [limit, setLimit] = useState(10);\n  let sort = \"price-asc\";\n\n  if (!price) {\n    sort = \"price-desc\";\n  }\n  console.log(sort);\n  // const numPages = Math.ceil(data.number / limit); // count the number of pages, adding buttons with pages number\n  console.log(search);\n\n  const fetchData = async () => {\n    try {\n      const response = await axios.get(\n        `https://vinted-reacteur.herokuapp.com/offers?page=${page}&title=${search}&sort=${sort}&priceMin=${range[0]}&priceMax=${range[1]}&limit=${limit}`\n      );\n      setData(response.data);\n      setIsLoading(false);\n      setMainPage(true);\n    } catch (error) {\n      console.log(error.message);\n      if (error.response) {\n        console.log(error.response.message);\n      }\n    }\n  };\n\n  console.log(data);\n\n  useEffect(() => {\n    fetchData();\n  }, [page, search, sort, range, limit]);\n\n  return isLoading ? (\n    <span>En cours de chargement</span>\n  ) : (\n    <div className=\"home\">\n      <div className=\"hero\">\n        <img src={hero} alt=\"Someone buying clothes\" />\n        <img className=\"overlay\" src={overlay} alt=\"overlay\" />\n        <div className=\"hero-box\">\n          <h3>Prêts à faire du tri dans vos placard ?</h3>\n          <Link to=\"/Publish\">Commencer à vendre</Link>\n        </div>\n      </div>\n      <div className=\"container\">\n        <div className=\"select\">\n          <span>Nombre de produits par page :</span>\n          <select\n            onChange={(event) => {\n              setLimit(event.target.value);\n            }}\n          >\n            <option value=\"10\">10</option>\n            <option value=\"25\">25</option>\n            <option value=\"50\">50</option>\n          </select>\n        </div>\n        <div className=\"products\">\n          {data.offers.map((el) => {\n            return <Product data={el} key={el._id} />;\n          })}\n        </div>\n      </div>\n      <div className=\"pages\">\n        {/* to improve with number of page */}\n        {page > 1 ? (\n          <button\n            onClick={() => {\n              setPage(page - 1);\n            }}\n          >\n            <FontAwesomeIcon icon={faChevronLeft} style={{ fontSize: 20, marginRight: 20 }} />\n            Précedent\n          </button>\n        ) : (\n          <div className=\"first-hidden\"></div>\n        )}\n        {data.offers.length < limit ? (\n          <div className=\"second-hidden\"></div>\n        ) : (\n          <button\n            onClick={() => {\n              setPage(page + 1);\n            }}\n          >\n            Suivant\n            <FontAwesomeIcon icon={faChevronRight} style={{ fontSize: 20, marginLeft: 20 }} />\n          </button>\n        )}\n      </div>\n    </div>\n  );\n};\nexport default Home;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Offer.js",["58"],"import { useParams } from \"react-router-dom\";\nimport { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport ProductDetails from \"../components/ProductDetails\";\nimport Carousel from \"react-multi-carousel\";\nimport \"react-multi-carousel/lib/styles.css\";\n\nconst Offer = ({ setMainPage }) => {\n  const { id } = useParams();\n  const [data, setData] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n\n  const fetchData = async () => {\n    try {\n      const response = await axios.get(`https://vinted-reacteur.herokuapp.com/offer/${id}`);\n      setData(response.data);\n      setIsLoading(false);\n      setMainPage(false);\n    } catch (error) {\n      console.log(error.message);\n      if (error.response) {\n        console.log(error.response.message);\n      }\n    }\n  };\n\n  useEffect(() => {\n    fetchData();\n  }, [id]);\n\n  const responsive = {\n    allTypeOfScreen: {\n      breakpoint: { max: 4000, min: 0 },\n      items: 1,\n    },\n  };\n\n  return isLoading ? (\n    <span>Chargement en cours...</span>\n  ) : (\n    <div className=\"offer\">\n      <div className=\"wrapper-offer\">\n        <Carousel responsive={responsive}>\n          {data.product_pictures.map((el) => {\n            return <img src={el.secure_url} alt={data.product_name} />;\n          })}\n        </Carousel>\n        <div className=\"offer-infos\">\n          <h4>{data.product_price.toFixed(2)} €</h4>\n          <ul className=\"product-details\">\n            {data.product_details.map((el, index) => {\n              return <ProductDetails data={el} key={index} />;\n            })}\n          </ul>\n          <div className=\"other-infos\">\n            <h5>{data.product_name}</h5>\n            <span>{data.product_description}</span>\n            <div>\n              {data.owner.account.avatar ? (\n                <img className=\"profil-picture\" src={data.owner.account.avatar.secure_url} alt=\"User's avatar\" />\n              ) : (\n                <div style={{ marginRight: 10 }} className=\"userpic\">\n                  <span style={{ color: \"#fff\", fontSize: 20 }}>{data.owner.account.username.substring(0, 1)}</span>\n                </div>\n              )}\n              <span>{data.owner.account.username}</span>\n            </div>\n            <button>Acheter</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Offer;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Header.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Product.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/compone