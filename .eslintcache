ingCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"32"},"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/index.js",[],["62","63"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/App.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Home.js",["64"],"import Product from \"../components/Product\";\nimport hero from \"../assets/img/Hero-picture.jpeg\";\nimport overlay from \"../assets/img/overlay.svg\";\nimport { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport axios from \"axios\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faChevronLeft, faChevronRight } from \"@fortawesome/free-solid-svg-icons\";\n\nconst Home = ({ search, price, range, setCurrentPage, server }) => {\n  const [data, setData] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n  // Intialization of queries\n  const [page, setPage] = useState(1);\n  const [limit, setLimit] = useState(10);\n  let sort = \"price-asc\";\n\n  // state price is a toggle defined by the button filter\n  if (!price) {\n    sort = \"price-desc\";\n  }\n\n  const fetchData = async () => {\n    try {\n      const response = await axios.get(\n        `${server}offers?page=${page}&title=${search}&sort=${sort}&priceMin=${range[0]}&priceMax=${range[1]}&limit=${limit}`\n      );\n      setData(response.data);\n      setIsLoading(false);\n      // Change current page to Home\n      setCurrentPage(\"Home\");\n    } catch (error) {\n      console.log(error.message);\n      if (error.response) {\n        console.log(error.response.message);\n      }\n    }\n  };\n\n  // Recharge the component when filters change\n  useEffect(() => {\n    fetchData();\n  }, [page, search, sort, range, limit]);\n\n  return isLoading ? (\n    <span>En cours de chargement</span>\n  ) : (\n    <div className=\"home\">\n      <div className=\"hero\">\n        <img src={hero} alt=\"Someone buying clothes\" />\n        <img className=\"overlay\" src={overlay} alt=\"overlay\" />\n        <div className=\"hero-box\">\n          <h3>Prêts à faire du tri dans vos placard ?</h3>\n          <Link to=\"/Publish\">Commencer à vendre</Link>\n        </div>\n      </div>\n      <div className=\"container\">\n        <div className=\"select\">\n          {/* Define the number of product in home page */}\n          <span>Nombre de produits par page :</span>\n          <select\n            onChange={(event) => {\n              setLimit(event.target.value);\n            }}\n          >\n            <option value=\"10\">10</option>\n            <option value=\"25\">25</option>\n            <option value=\"50\">50</option>\n          </select>\n        </div>\n        <div className=\"products\">\n          {data.offers.map((el) => {\n            return <Product data={el} key={el._id} />;\n          })}\n        </div>\n      </div>\n      {/* For changing page */}\n      <div className=\"pages\">\n        {page > 1 ? (\n          <button\n            onClick={() => {\n              setPage(page - 1);\n            }}\n          >\n            <FontAwesomeIcon icon={faChevronLeft} style={{ fontSize: 20, marginRight: 20 }} />\n            Précedent\n          </button>\n        ) : (\n          <div className=\"first-hidden\"></div>\n        )}\n        {data.offers.length < limit ? (\n          <div className=\"second-hidden\"></div>\n        ) : (\n          <button\n            onClick={() => {\n              setPage(page + 1);\n            }}\n          >\n            Suivant\n            <FontAwesomeIcon icon={faChevronRight} style={{ fontSize: 20, marginLeft: 20 }} />\n          </button>\n        )}\n      </div>\n    </div>\n  );\n};\nexport default Home;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Offer.js",["65"],"import { useParams, useHistory } from \"react-router-dom\";\nimport { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport ProductDetails from \"../components/ProductDetails\";\nimport Carousel from \"react-multi-carousel\";\nimport \"react-multi-carousel/lib/styles.css\";\nimport Cookies from \"js-cookie\";\nimport Login from \"../components/Login\";\n\nconst Offer = ({ setCurrentPage, loginModal, setLoginModal, setSignupModal, currentPage, setUser, server }) => {\n  const { id } = useParams();\n  const [data, setData] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n  const history = useHistory();\n  const token = Cookies.get(\"userToken\");\n\n  const fetchData = async () => {\n    try {\n      const response = await axios.get(`${server}offer/${id}`);\n      setData(response.data);\n      setIsLoading(false);\n      // Define current page on Offer/id of target product\n      setCurrentPage(`Offer/${id}`);\n    } catch (error) {\n      console.log(error.message);\n      if (error.response) {\n        console.log(error.response.message);\n      }\n    }\n  };\n\n  useEffect(() => {\n    fetchData();\n  }, [id]);\n\n  // Responsive for carousel\n  const responsive = {\n    allTypeOfScreen: {\n      breakpoint: { max: 4000, min: 0 },\n      items: 1,\n    },\n  };\n\n  return isLoading ? (\n    <span>Chargement en cours...</span>\n  ) : (\n    <div className=\"offer\">\n      <div className=\"wrapper-offer\">\n        {/* Display each picture of the product with a carousel */}\n        <Carousel responsive={responsive}>\n          {data.product_pictures.map((el) => {\n            return <img src={el.secure_url} alt={data.product_name} />;\n          })}\n        </Carousel>\n        <div className=\"offer-infos\">\n          <h4>{data.product_price.toFixed(2)} €</h4>\n          <ul className=\"product-details\">\n            {/* Return details of the product */}\n            {data.product_details.map((el, index) => {\n              return <ProductDetails data={el} key={index} />;\n            })}\n          </ul>\n          <div className=\"other-infos\">\n            <h5>{data.product_name}</h5>\n            <span>{data.product_description}</span>\n            <div>\n              {/* If owner hasn't avatar, create a fake avatar with the first letter of username in uppercase in a circle */}\n              {data.owner.account.avatar ? (\n                <img className=\"profil-picture\" src={data.owner.account.avatar.secure_url} alt=\"User's avatar\" />\n              ) : (\n                <div style={{ marginRight: 10 }} className=\"userpic\">\n                  <span style={{ color: \"#fff\", fontSize: 20 }}>{data.owner.account.username.substring(0, 1).toUpperCase()}</span>\n                </div>\n              )}\n              <span>{data.owner.account.username}</span>\n            </div>\n            <button\n              // Check if user has a token\n              onClick={() => {\n                if (token) {\n                  history.push(\"/Payment\", { title: data.product_name, price: data.product_price });\n                } else {\n                  <Login\n                    setUser={setUser}\n                    setLoginModal={setLoginModal}\n                    setSignupModal={setSignupModal}\n                    currentPage={currentPage}\n                    server={server}\n                  />;\n                  setLoginModal((loginModal) => !loginModal);\n                }\n              }}\n            >\n              Acheter\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Offer;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Header.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Product.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/ProductDetails.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/PriceSwitch.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/PriceRange.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Signup.js",["66"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Login.js",["67"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Publish.js",["68","69"],"import axios from \"axios\";\nimport { useState, useEffect } from \"react\";\nimport Cookies from \"js-cookie\";\nimport Login from \"../components/Login\";\nimport { Link } from \"react-router-dom\";\n\nconst Publish = ({ setCurrentPage, setUser, setLoginModal, setSignupModal, currentPage, server }) => {\n  const [data, setData] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n  // State for data to send to server\n  const [file, setFile] = useState();\n  const [title, setTitle] = useState(\"\");\n  const [description, setDescription] = useState(\"\");\n  const [brand, setBrand] = useState(\"\");\n  const [size, setSize] = useState(\"\");\n  const [color, setColor] = useState(\"\");\n  const [condition, setCondition] = useState(\"\");\n  const [location, setLocation] = useState(\"\");\n  const [price, setPrice] = useState(\"\");\n  const [preview, setPreview] = useState(\"\");\n  const token = Cookies.get(\"userToken\");\n\n  const handleSubmit = async (event) => {\n    try {\n      event.preventDefault();\n\n      const formData = new FormData();\n      formData.append(\"picture\", file);\n      formData.append(\"title\", title);\n      formData.append(\"description\", description);\n      formData.append(\"brand\", brand);\n      formData.append(\"size\", size);\n      formData.append(\"color\", color);\n      formData.append(\"condition\", condition);\n      formData.append(\"location\", location);\n      formData.append(\"price\", price);\n\n      const response = await axios.post(`${server}offer/publish`, formData, {\n        headers: {\n          authorization: `Bearer ${token}`,\n        },\n      });\n      setData(response.data);\n      setIsLoading(false);\n    } catch (error) {\n      console.log(error.message);\n    }\n  };\n\n  // Check if user is logged and has token\n  const checkLogin = () => {\n    if (!token) {\n      <Login setUser={setUser} setLoginModal={setLoginModal} setSignupModal={setSignupModal} currentPage={currentPage} />;\n      setLoginModal((loginModal) => !loginModal);\n    }\n  };\n\n  useEffect(() => {\n    checkLogin();\n  }, []);\n\n  return (\n    <div className=\"publish\">\n      {setCurrentPage(\"Publish\")}\n      <div className=\"container\">\n        {isLoading ? (\n          <>\n            <h2>Vends ton article</h2>\n            <form onSubmit={handleSubmit}>\n              <div className=\"publish-wrapper\">\n                <div className=\"dashed\">\n                  {/* Preview the image dropped by the user -- drag'n drog to come  */}\n                  {preview ? (\n                    <div style={{ border: \"none\", width: \"100%\", height: \"100%\", display: \"flex\", justifyContent: \"flex-start\" }}>\n                      <img src={preview} alt=\"your picture\" style={{ height: 190, width: 150 }} />\n                    </div>\n                  ) : (\n                    <div>\n                      <label htmlFor=\"pictures\">\n                        <span>+</span>\n                        <span>Ajoute une photo</span>\n                      </label>\n                      <input\n                        type=\"file\"\n                        id=\"pictures\"\n                        onChange={(event) => {\n                          setFile(event.target.files[0]);\n                          setPreview(URL.createObjectURL(event.target.files[0]));\n                        }}\n                      />\n                    </div>\n                  )}\n                </div>\n              </div>\n              <div>\n                <div>\n                  <label htmlFor=\"title\">Titre</label>\n                  <input\n                    type=\"text\"\n                    id=\"title\"\n                    value={title}\n                    placeholder=\"ex: Chemise Sézane verte\"\n                    onChange={(event) => {\n                      setTitle(event.target.value);\n                    }}\n                  />\n                </div>\n                <div className=\"description-box\">\n                  <label htmlFor=\"description\">Décris ton article</label>\n                  <textarea\n                    type=\"text\"\n                    id=\"description\"\n                    value={description}\n                    placeholder=\"ex: porté quelques fois, taille correctement\"\n                    onChange={(event) => {\n                      setDescription(event.target.value);\n                    }}\n                  />\n                </div>\n              </div>\n              <div>\n                <div>\n                  <label htmlFor=\"brand\">Marque</label>\n                  <input\n                    type=\"text\"\n                    id=\"brand\"\n                    value={brand}\n                    placeholder=\"ex: Sézanne\"\n                    onChange={(event) => {\n                      setBrand(event.target.value);\n                    }}\n                  />\n                </div>\n                <div>\n                  <label htmlFor=\"size\">Taille</label>\n                  <input\n                    type=\"text\"\n                    id=\"size\"\n                    value={size}\n                    placeholder=\"ex: L / 40 / 12\"\n                    onChange={(event) => {\n                      setSize(event.target.value);\n                    }}\n                  />\n                </div>\n                <div>\n                  <label htmlFor=\"color\">Couleur</label>\n                  <input\n                    type=\"text\"\n                    id=\"color\"\n                    value={color}\n                    placeholder=\"ex: Vert\"\n                    onChange={(event) => {\n                      setColor(event.target.value);\n                    }}\n                  />\n                </div>\n                <div>\n                  <label htmlFor=\"condition\">Etat</label>\n                  <input\n                    type=\"text\"\n                    id=\"condition\"\n                    value={condition}\n                    placeholder=\"ex: Neuf avec étiquette\"\n                    onChange={(event) => {\n                      setCondition(event.target.value);\n                    }}\n                  />\n                </div>\n                <div>\n                  <label htmlFor=\"location\">Lieu</label>\n                  <input\n                    type=\"text\"\n                    id=\"location\"\n                    value={location}\n                    placeholder=\"ex: Paris\"\n                    onChange={(event) => {\n                      setLocation(event.target.value);\n                    }}\n                  />\n                </div>\n              </div>\n              <div>\n                <div>\n                  <label htmlFor=\"price\">Prix</label>\n                  <div className=\"price-checkbox\">\n                    <input\n                      type=\"text\"\n                      id=\"price\"\n                      value={price}\n                      placeholder=\"0,00 €\"\n                      onChange={(event) => {\n                        setPrice(event.target.value);\n                      }}\n                    />\n                    <div>\n                      <input type=\"checkbox\" id=\"swap\" />\n                      <label htmlFor=\"swap\">Je suis intéressé(e) par les échanges</label>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <div className=\"button-div\">\n                <button>Ajouter</button>\n              </div>\n            </form>\n          </>\n        ) : (\n          // User can choose between go back to home page or check his offer after submit\n          <div className=\"after-submit\">\n            <div>\n              <h3>Ton offre a bien été créée</h3>\n              <div>\n                <Link to={`/Offer/${data._id}`}>Voir ton offre</Link>\n                <Link to=\"/\">Page d'accueil</Link>\n              </div>\n            </div>\n          </div>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Publish;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Payment.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/CheckoutForm.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":43,"column":6,"nodeType":"76","endLine":43,"endColumn":40,"suggestions":"77"},{"ruleId":"74","severity":1,"message":"75","line":34,"column":6,"nodeType":"76","endLine":34,"endColumn":10,"suggestions":"78"},{"ruleId":"79","severity":1,"message":"80","line":96,"column":11,"nodeType":"81","endLine":102,"endColumn":12},{"ruleId":"79","severity":1,"message":"80","line":68,"column":11,"nodeType":"81","endLine":74,"endColumn":12},{"ruleId":"74","severity":1,"message":"82","line":60,"column":6,"nodeType":"76","endLine":60,"endColumn":8,"suggestions":"83"},{"ruleId":"84","severity":1,"message":"85","line":75,"column":23,"nodeType":"81","endLine":75,"endColumn":99},"no-native-reassign",["86"],"no-negated-in-lhs",["87"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'fetchData'. Either include it or remove the dependency array.","ArrayExpression",["88"],["89"],"jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Provide a valid, navigable address as the href value. If you cannot provide an href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anch