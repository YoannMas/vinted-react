fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/index.js",[],["61","62"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/App.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Home.js",["63"],"import Product from \"../components/Product\";\nimport hero from \"../assets/img/Hero-picture.jpeg\";\nimport overlay from \"../assets/img/overlay.svg\";\nimport { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport axios from \"axios\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faChevronLeft, faChevronRight } from \"@fortawesome/free-solid-svg-icons\";\n\nconst Home = ({ search, price, range, setCurrentPage }) => {\n  const [data, setData] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n  const [page, setPage] = useState(1);\n  const [limit, setLimit] = useState(10);\n  let sort = \"price-asc\";\n\n  if (!price) {\n    sort = \"price-desc\";\n  }\n  // const numPages = Math.ceil(data.number / limit); // count the number of pages, adding buttons with pages number\n\n  const fetchData = async () => {\n    try {\n      const response = await axios.get(\n        `https://vinted-reacteur.herokuapp.com/offers?page=${page}&title=${search}&sort=${sort}&priceMin=${range[0]}&priceMax=${range[1]}&limit=${limit}`\n      );\n      setData(response.data);\n      setIsLoading(false);\n      setCurrentPage(\"Home\");\n    } catch (error) {\n      console.log(error.message);\n      if (error.response) {\n        console.log(error.response.message);\n      }\n    }\n  };\n\n  useEffect(() => {\n    fetchData();\n  }, [page, search, sort, range, limit]);\n\n  return isLoading ? (\n    <span>En cours de chargement</span>\n  ) : (\n    <div className=\"home\">\n      <div className=\"hero\">\n        <img src={hero} alt=\"Someone buying clothes\" />\n        <img className=\"overlay\" src={overlay} alt=\"overlay\" />\n        <div className=\"hero-box\">\n          <h3>Prêts à faire du tri dans vos placard ?</h3>\n          <Link to=\"/Publish\">Commencer à vendre</Link>\n        </div>\n      </div>\n      <div className=\"container\">\n        <div className=\"select\">\n          <span>Nombre de produits par page :</span>\n          <select\n            onChange={(event) => {\n              setLimit(event.target.value);\n            }}\n          >\n            <option value=\"10\">10</option>\n            <option value=\"25\">25</option>\n            <option value=\"50\">50</option>\n          </select>\n        </div>\n        <div className=\"products\">\n          {data.offers.map((el) => {\n            return <Product data={el} key={el._id} />;\n          })}\n        </div>\n      </div>\n      <div className=\"pages\">\n        {/* to improve with number of page */}\n        {page > 1 ? (\n          <button\n            onClick={() => {\n              setPage(page - 1);\n            }}\n          >\n            <FontAwesomeIcon icon={faChevronLeft} style={{ fontSize: 20, marginRight: 20 }} />\n            Précedent\n          </button>\n        ) : (\n          <div className=\"first-hidden\"></div>\n        )}\n        {data.offers.length < limit ? (\n          <div className=\"second-hidden\"></div>\n        ) : (\n          <button\n            onClick={() => {\n              setPage(page + 1);\n            }}\n          >\n            Suivant\n            <FontAwesomeIcon icon={faChevronRight} style={{ fontSize: 20, marginLeft: 20 }} />\n          </button>\n        )}\n      </div>\n    </div>\n  );\n};\nexport default Home;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Offer.js",["64"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Header.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Product.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/ProductDetails.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/PriceSwitch.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/PriceRange.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Signup.js",["65"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Login.js",["66"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Publish.js",["67","68"],"import axios from \"axios\";\nimport { useState, useEffect } from \"react\";\nimport Cookies from \"js-cookie\";\nimport Login from \"../components/Login\";\nimport { Link } from \"react-router-dom\";\n\nconst Publish = ({ setCurrentPage, setUser, setLoginModal, setSignupModal, currentPage }) => {\n  const [data, setData] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n  const [file, setFile] = useState();\n  const [title, setTitle] = useState(\"\");\n  const [description, setDescription] = useState(\"\");\n  const [brand, setBrand] = useState(\"\");\n  const [size, setSize] = useState(\"\");\n  const [color, setColor] = useState(\"\");\n  const [condition, setCondition] = useState(\"\");\n  const [location, setLocation] = useState(\"\");\n  const [price, setPrice] = useState(\"\");\n  const token = Cookies.get(\"userToken\");\n  const [preview, setPreview] = useState(\"\");\n\n  const handleSubmit = async (event) => {\n    try {\n      event.preventDefault();\n\n      const formData = new FormData();\n      formData.append(\"picture\", file);\n      formData.append(\"title\", title);\n      formData.append(\"description\", description);\n      formData.append(\"brand\", brand);\n      formData.append(\"size\", size);\n      formData.append(\"color\", color);\n      formData.append(\"condition\", condition);\n      formData.append(\"location\", location);\n      formData.append(\"price\", price);\n\n      const response = await axios.post(\"https://vinted-reacteur.herokuapp.com/offer/publish\", formData, {\n        headers: {\n          authorization: `Bearer ${token}`,\n        },\n      });\n      setData(response.data);\n      setIsLoading(false);\n    } catch (error) {\n      console.log(error.message);\n    }\n  };\n\n  const checkLogin = () => {\n    if (!token) {\n      <Login setUser={setUser} setLoginModal={setLoginModal} setSignupModal={setSignupModal} currentPage={currentPage} />;\n      setLoginModal((loginModal) => !loginModal);\n    }\n  };\n\n  useEffect(() => {\n    checkLogin();\n  }, []);\n\n  return (\n    <div className=\"publish\">\n      {setCurrentPage(\"Publish\")}\n      <div className=\"container\">\n        {isLoading ? (\n          <>\n            <h2>Vends ton article</h2>\n            <form onSubmit={handleSubmit}>\n              <div className=\"publish-wrapper\">\n                <div className=\"dashed\">\n                  {preview ? (\n                    <div style={{ border: \"none\", width: \"100%\", height: \"100%\", display: \"flex\", justifyContent: \"flex-start\" }}>\n                      <img src={preview} alt=\"your picture\" style={{ height: 190, width: 150 }} />\n                    </div>\n                  ) : (\n                    <div>\n                      <label htmlFor=\"pictures\">\n                        <span>+</span>\n                        <span>Ajoute une photo</span>\n                      </label>\n                      <input\n                        type=\"file\"\n                        id=\"pictures\"\n                        onChange={(event) => {\n                          setFile(event.target.files[0]);\n                          setPreview(URL.createObjectURL(event.target.files[0]));\n                        }}\n                      />\n                    </div>\n                  )}\n                </div>\n              </div>\n              <div>\n                <div>\n                  <label htmlFor=\"title\">Titre</label>\n                  <input\n                    type=\"text\"\n                    id=\"title\"\n                    value={title}\n                    placeholder=\"ex: Chemise Sézane verte\"\n                    onChange={(event) => {\n                      setTitle(event.target.value);\n                    }}\n                  />\n                </div>\n                <div className=\"description-box\">\n                  <label htmlFor=\"description\">Décris ton article</label>\n                  <textarea\n                    type=\"text\"\n                    id=\"description\"\n                    value={description}\n                    placeholder=\"ex: porté quelques fois, taille correctement\"\n                    onChange={(event) => {\n                      setDescription(event.target.value);\n                    }}\n                  />\n                </div>\n              </div>\n              <div>\n                <div>\n                  <label htmlFor=\"brand\">Marque</label>\n                  <input\n                    type=\"text\"\n                    id=\"brand\"\n                    value={brand}\n                    placeholder=\"ex: Sézanne\"\n                    onChange={(event) => {\n                      setBrand(event.target.value);\n                    }}\n                  />\n                </div>\n                <div>\n                  <label htmlFor=\"size\">Taille</label>\n                  <input\n                    type=\"text\"\n                    id=\"size\"\n                    value={size}\n                    placeholder=\"ex: L / 40 / 12\"\n                    onChange={(event) => {\n                      setSize(event.target.value);\n                    }}\n                  />\n                </div>\n                <div>\n                  <label htmlFor=\"color\">Couleur</label>\n                  <input\n                    type=\"text\"\n                    id=\"color\"\n                    value={color}\n                    placeholder=\"ex: Vert\"\n                    onChange={(event) => {\n                      setColor(event.target.value);\n                    }}\n                  />\n                </div>\n                <div>\n                  <label htmlFor=\"condition\">Etat</label>\n                  <input\n                    type=\"text\"\n                    id=\"condition\"\n                    value={condition}\n                    placeholder=\"ex: Neuf avec étiquette\"\n                    onChange={(event) => {\n                      setCondition(event.target.value);\n                    }}\n                  />\n                </div>\n                <div>\n                  <label htmlFor=\"location\">Lieu</label>\n                  <input\n                    type=\"text\"\n                    id=\"location\"\n                    value={location}\n                    placeholder=\"ex: Paris\"\n                    onChange={(event) => {\n                      setLocation(event.target.value);\n                    }}\n                  />\n                </div>\n              </div>\n              <div>\n                <div>\n                  <label htmlFor=\"price\">Prix</label>\n                  <div className=\"price-checkbox\">\n                    <input\n                      type=\"text\"\n                      id=\"price\"\n                      value={price}\n                      placeholder=\"0,00 €\"\n                      onChange={(event) => {\n                        setPrice(event.target.value);\n                      }}\n                    />\n                    <div>\n                      <input type=\"checkbox\" id=\"swap\" />\n                      <label htmlFor=\"swap\">Je suis intéressé(e) par les échanges</label>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <div className=\"button-div\">\n                <button>Ajouter</button>\n              </div>\n            </form>\n          </>\n        ) : (\n          <div className=\"after-submit\">\n            <div>\n              <h3>Ton offre a bien été créée</h3>\n              <div>\n                <Link to={`/Offer/${data._id}`}>Voir ton offre</Link>\n                <Link to=\"/\">Page d'accueil</Link>\n              </div>\n            </div>\n          </div>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Publish;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Payment.js",["69","70","71"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/CheckoutForm.js",["72","73","74"],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":40,"column":6,"nodeType":"81","endLine":40,"endColumn":40,"suggestions":"82"},{"ruleId":"79","severity":1,"message":"80","line":33,"column":6,"nodeType":"81","endLine":33,"endColumn":10,"suggestions":"83"},{"ruleId":"84","severity":1,"message":"85","line":95,"column":11,"nodeType":"86","endLine":100,"endColumn":12},{"ruleId":"84","severity":1,"message":"85","line":67,"column":11,"nodeType":"86","endLine":72,"endColumn":12},{"ruleId":"79","severity":1,"message":"87","line":58,"column":6,"nodeType":"81","endLine":58,"endColumn":8,"suggestions":"88"},{"ruleId":"89","severity":1,"message":"90","line":72,"column":23,"nodeType":"86","endLine":72,"endColumn":99},{"ruleId":"91","severity":1,"message":"92","line":5,"column":8,"nodeType":"93","messageId":"94","endLine":5,"endColumn":13},{"ruleId":"91","severity":1,"message":"95","line":6,"column":8,"nodeType":"93","messageId":"94","endLine":6,"endColumn":15},{"ruleId":"91","severity":1,"message":"96","line":7,"column":10,"nodeType":"93","messageId":"94","endLine":7,"endColumn":19},{"ruleId":"91","severity":1,"message":"97","line":1,"column":10,"nodeType":"93","messageId":"94","endLine":1,"endColumn":19},{"ruleId":"91","severity":1,"message":"98","line":1,"column":21,"nodeType":"93","messageId":"94","endLine":1