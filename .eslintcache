":"61","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"32"},"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/index.js",[],["62","63"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/App.js",[],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Home.js",["64"],"import Product from \"../components/Product\";\nimport hero from \"../assets/img/Hero-picture.jpeg\";\nimport overlay from \"../assets/img/overlay.svg\";\nimport { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport axios from \"axios\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faChevronLeft, faChevronRight } from \"@fortawesome/free-solid-svg-icons\";\n\nconst Home = ({ search, price, range, setCurrentPage, server }) => {\n  const [data, setData] = useState({});\n  const [isLoading, setIsLoading] = useState(true);\n  // Intialization of queries\n  const [page, setPage] = useState(1);\n  const [limit, setLimit] = useState(10);\n  let sort = \"price-asc\";\n\n  // state price is a toggle defined by the button filter\n  if (!price) {\n    sort = \"price-desc\";\n  }\n\n  const fetchData = async () => {\n    try {\n      const response = await axios.get(\n        `${server}offers?page=${page}&title=${search}&sort=${sort}&priceMin=${range[0]}&priceMax=${range[1]}&limit=${limit}`\n      );\n      setData(response.data);\n      setIsLoading(false);\n      // Change current page to Home\n      setCurrentPage(\"Home\");\n    } catch (error) {\n      console.log(error.message);\n      if (error.response) {\n        console.log(error.response.message);\n      }\n    }\n  };\n\n  // Recharge the component when filters change\n  useEffect(() => {\n    fetchData();\n  }, [page, search, sort, range, limit]);\n\n  return isLoading ? (\n    <span>En cours de chargement</span>\n  ) : (\n    <div className=\"home\">\n      <div className=\"hero\">\n        <img src={hero} alt=\"Someone buying clothes\" />\n        <img className=\"overlay\" src={overlay} alt=\"overlay\" />\n        <div className=\"hero-box\">\n          <h3>Prêts à faire du tri dans vos placard ?</h3>\n          <Link to=\"/Publish\">Commencer à vendre</Link>\n        </div>\n      </div>\n      <div className=\"container\">\n        <div className=\"select\">\n          {/* Define the number of product in home page */}\n          <span>Nombre de produits par page :</span>\n          <select\n            onChange={(event) => {\n              setLimit(event.target.value);\n            }}\n          >\n            <option value=\"10\">10</option>\n            <option value=\"25\">25</option>\n            <option value=\"50\">50</option>\n          </select>\n        </div>\n        <div className=\"products\">\n          {data.offers.map((el) => {\n            return <Product data={el} key={el._id} />;\n          })}\n        </div>\n      </div>\n      {/* For changing page */}\n      <div className=\"pages\">\n        {page > 1 ? (\n          <button\n            onClick={() => {\n              setPage(page - 1);\n            }}\n          >\n            <FontAwesomeIcon icon={faChevronLeft} style={{ fontSize: 20, marginRight: 20 }} />\n            Précedent\n          </button>\n        ) : (\n          <div className=\"first-hidden\"></div>\n        )}\n        {data.offers.length < limit ? (\n          <div className=\"second-hidden\"></div>\n        ) : (\n          <button\n            onClick={() => {\n              setPage(page + 1);\n            }}\n          >\n            Suivant\n            <FontAwesomeIcon icon={faChevronRight} style={{ fontSize: 20, marginLeft: 20 }} />\n          </button>\n        )}\n      </div>\n    </div>\n  );\n};\nexport default Home;\n","/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/containers/Offer.js",["65","66"],"/Users/yoann/LeReacteur/projects/vinted/vinted-front/src/components/Header.js",[],"/Users/yoa